<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        https://symfony.com/schema/routing/routing-1.0.xsd">


    <route id="digital_ascetic_base_user_security_login" path="/login" methods="GET POST">
        <default key="_controller">digital_ascetic.base_user.security_controller::login</default>
    </route>

    <route id="digital_ascetic_base_user_security_check" path="/login_check" methods="POST">
        <default key="_controller">digital_ascetic.base_user.security_controller::check</default>
    </route>

    <route id="digital_ascetic_base_user_security_logout" path="/logout" methods="GET POST">
        <default key="_controller">digital_ascetic.base_user.security_controller::logout</default>
    </route>
</routes>
