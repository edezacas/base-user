{
    "name": "edezacas/base-user",
    "version": "2.0.2",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Provides user management for your Symfony 5 project.",
    "authors": [
        {
            "name": "Eduard Deza",
            "email": "edezacas@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/form": "*",
        "symfony/orm-pack": "*",
        "symfony/security-bundle": "*",
        "symfony/twig-bundle": "*"
    },
    "require-dev": {
        "jms/serializer-bundle": "^3.7",
        "symfony/debug": "^4.4",
        "symfony/test-pack": "^1.0",
        "symfony/yaml": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EDC\\BaseUserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EDC\\BaseUserBundle\\Tests\\": "tests/"
        }
    }
}
