{
    "name": "digitalascetic/base-user",
    "version": "1.0.0",
    "type": "symfony-bundle",
    "license": "proprietary",
    "description": "Digital Ascetic Base User Bundle",
    "authors": [
        {
            "name": "Eduard Deza",
            "email": "ed@ascetic.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "symfony/form": "^4.0|^5.0",
        "symfony/framework-bundle": "^4.0|^5.0",
        "symfony/orm-pack": "2.0.0",
        "symfony/security-bundle": "^4.0|^5.0"
    },
    "require-dev": {
        "jms/serializer-bundle": "^3.7",
        "symfony/debug": "^4.4",
        "symfony/test-pack": "^1.0",
        "symfony/twig-bundle": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DigitalAscetic\\BaseUserBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalAscetic\\BaseUserBundle\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
